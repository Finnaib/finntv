{
    "buildCommand": "echo 'No build required'",
    "outputDirectory": ".",
    "installCommand": "echo 'No dependencies to install'",
    "functions": {
        "api/player_api.php": {
            "runtime": "vercel-php@0.7.1"
        },
        "api/get.php": {
            "runtime": "vercel-php@0.7.1"
        },
        "api/live.php": {
            "runtime": "vercel-php@0.7.1"
        },
        "api/panel_api.php": {
            "runtime": "vercel-php@0.7.1"
        },
        "api/xmltv.php": {
            "runtime": "vercel-php@0.7.1"
        },
        "api/debug.php": {
            "runtime": "vercel-php@0.7.1"
        }
    },
    "rewrites": [
        {
            "source": "/player_api.php",
            "destination": "/api/player_api"
        },
        {
            "source": "/get.php",
            "destination": "/api/get"
        },
        {
            "source": "/live/:path*",
            "destination": "/api/live"
        },
        {
            "source": "/movie/:path*",
            "destination": "/api/live"
        },
        {
            "source": "/series/:path*",
            "destination": "/api/live"
        },
        {
            "source": "/panel_api.php",
            "destination": "/api/panel_api"
        },
        {
            "source": "/xmltv.php",
            "destination": "/api/xmltv"
        },
        {
            "source": "/",
            "destination": "/index.html"
        }
    ]
}