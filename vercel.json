{
    "buildCommand": "echo 'No build required'",
    "outputDirectory": ".",
    "installCommand": "echo 'No dependencies to install'",
    "functions": {
        "*.php": {
            "runtime": "vercel-php@0.6.0"
        }
    },
    "routes": [
        {
            "src": "/player_api.php",
            "dest": "/player_api.php"
        },
        {
            "src": "/get.php",
            "dest": "/get.php"
        },
        {
            "src": "/live/(.*)",
            "dest": "/live.php"
        },
        {
            "src": "/panel_api.php",
            "dest": "/panel_api.php"
        },
        {
            "src": "/xmltv.php",
            "dest": "/xmltv.php"
        },
        {
            "src": "/m3u/(.*)",
            "dest": "/m3u/$1"
        },
        {
            "src": "/",
            "dest": "/index.html"
        }
    ],
    "cleanUrls": true,
    "trailingSlash": false
}