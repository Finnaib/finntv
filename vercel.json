{
    "version": 2,
    "builds": [
        {
            "src": "api/*.php",
            "use": "vercel-php@0.7.1",
            "config": {
                "includeFiles": [
                    "m3u/**",
                    "core/**",
                    "config.php"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/player_api.php",
            "dest": "/api/player_api.php"
        },
        {
            "src": "/get.php",
            "dest": "/api/get.php"
        },
        {
            "src": "/live/(.*)",
            "dest": "/api/live.php"
        },
        {
            "src": "/movie/(.*)",
            "dest": "/api/live.php"
        },
        {
            "src": "/series/(.*)",
            "dest": "/api/live.php"
        },
        {
            "src": "/panel_api.php",
            "dest": "/api/panel_api.php"
        },
        {
            "src": "/xmltv.php",
            "dest": "/api/xmltv.php"
        },
        {
            "src": "/debug",
            "dest": "/api/debug.php"
        },
        {
            "src": "/",
            "dest": "/index.html"
        }
    ]
}