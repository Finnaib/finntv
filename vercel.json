{
    "buildCommand": "echo 'No build required'",
    "outputDirectory": ".",
    "installCommand": "echo 'No dependencies to install'",
    "functions": {
        "api/player_api.php": {
            "runtime": "vercel-php@0.7.1"
        },
        "api/get.php": {
            "runtime": "vercel-php@0.7.1"
        },
        "api/live.php": {
            "runtime": "vercel-php@0.7.1"
        },
        "api/panel_api.php": {
            "runtime": "vercel-php@0.7.1"
        },
        "api/xmltv.php": {
            "runtime": "vercel-php@0.7.1"
        }
    },
    "routes": [
        {
            "src": "/player_api.php",
            "dest": "/api/player_api.php"
        },
        {
            "src": "/get.php",
            "dest": "/api/get.php"
        },
        {
            "src": "/live/(.*)",
            "dest": "/api/live.php"
        },
        {
            "src": "/panel_api.php",
            "dest": "/api/panel_api.php"
        },
        {
            "src": "/xmltv.php",
            "dest": "/api/xmltv.php"
        },
        {
            "src": "/m3u/(.*)",
            "dest": "/m3u/$1"
        },
        {
            "src": "/",
            "dest": "/index.html"
        }
    ]
}