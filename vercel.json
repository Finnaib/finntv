{
    "version": 2,
    "builds": [
        {
            "src": "api/*.php",
            "use": "vercel-php@0.6.0"
        }
    ],
    "routes": [
        {
            "src": "/player_api.php",
            "dest": "/api/player_api.php"
        },
        {
            "src": "/get.php",
            "dest": "/api/get.php"
        },
        {
            "src": "/live/(.*)",
            "dest": "/api/live.php"
        },
        {
            "src": "/movie/(.*)",
            "dest": "/api/live.php"
        },
        {
            "src": "/series/(.*)",
            "dest": "/api/live.php"
        },
        {
            "src": "/panel_api.php",
            "dest": "/api/panel_api.php"
        },
        {
            "src": "/xmltv.php",
            "dest": "/api/xmltv.php"
        },
        {
            "src": "/",
            "dest": "/index.html"
        }
    ]
}